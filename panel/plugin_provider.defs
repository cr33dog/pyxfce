    '("panel-preferences" "PROVIDER_SIGNAL_PANEL_PREFERENCES")
    '("panel-logout" "PROVIDER_SIGNAL_PANEL_LOGOUT")
    '("panel-about" "PROVIDER_SIGNAL_PANEL_ABOUT")
    '("panel-help" "PROVIDER_SIGNAL_PANEL_HELP")
